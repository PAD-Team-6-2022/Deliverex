<aside class="bg-white border-r border-r-slate-200 p-4 sticky top-0 h-screen flex flex-col justify-between z-40">
  <% if(typeof isShopOwner == "undefined") isShopOwner = true %>
  <%# Home %>
  <div class="space-y-3">
    <a href="/dashboard/overview" class="p-2 bg-cyan-500 text-white rounded-md hover:bg-cyan-700 transition-colors block" data-tooltip="Overview" data-tooltip-orientation="right" data-tooltip-offset="10">
      <svg viewBox="-2 -2 24 24" class="h-6 w-6" fill="currentColor">
        <path d="M18 18V7.132l-8-4.8-8 4.8V18h4v-2.75a4 4 0 1 1 8 0V18h4zm-6 2v-4.75a2 2 0 1 0-4 0V20H2a2 2 0 0 1-2-2V7.132a2 2 0 0 1 .971-1.715l8-4.8a2 2 0 0 1 2.058 0l8 4.8A2 2 0 0 1 20 7.132V18a2 2 0 0 1-2 2h-6z"></path>
      </svg>
    </a>
    <%# Orders %>
    <% if(isShopOwner) { %>
    <div class="border-t border-slate-200 h-px"></div>
    <a href="/dashboard/orders" class="p-2 bg-slate-100 hover:bg-slate-200 rounded-md transition-colors block" data-tooltip="Orders" data-tooltip-orientation="right" data-tooltip-offset="10">
      <svg viewBox="-2 -4 24 24" class="h-6 w-6" fill="currentColor">
        <path d="M3 0h14a3 3 0 0 1 3 3v10a3 3 0 0 1-3 3H3a3 3 0 0 1-3-3V3a3 3 0 0 1 3-3zm0 2a1 1 0 0 0-1 1v10a1 1 0 0 0 1 1h14a1 1 0 0 0 1-1V3a1 1 0 0 0-1-1H3zm10.874 5a4.002 4.002 0 0 1-7.748 0H2V5h16v2h-4.126zm-2.142 0H8.268a2 2 0 0 0 3.464 0z"></path>
      </svg>
    </a>
    <%# Users %>
    <a href="/dashboard/users" class="p-2 bg-slate-100 hover:bg-slate-200 rounded-md transition-colors block" data-tooltip="Users" data-tooltip-orientation="right" data-tooltip-offset="10">
      <svg viewBox="-2 -1.5 24 24" class="h-6 w-6" fill="currentColor">
        <path d="M3.534 11.07a1 1 0 1 1 .733 1.86A3.579 3.579 0 0 0 2 16.26V18a1 1 0 0 0 1 1h8a1 1 0 0 0 1-1v-1.647a3.658 3.658 0 0 0-2.356-3.419 1 1 0 1 1 .712-1.868A5.658 5.658 0 0 1 14 16.353V18a3 3 0 0 1-3 3H3a3 3 0 0 1-3-3v-1.74a5.579 5.579 0 0 1 3.534-5.19zM7 1a4 4 0 0 1 4 4v2a4 4 0 1 1-8 0V5a4 4 0 0 1 4-4zm0 2a2 2 0 0 0-2 2v2a2 2 0 1 0 4 0V5a2 2 0 0 0-2-2zm9 17a1 1 0 0 1 0-2h1a1 1 0 0 0 1-1v-1.838a3.387 3.387 0 0 0-2.316-3.213 1 1 0 1 1 .632-1.898A5.387 5.387 0 0 1 20 15.162V17a3 3 0 0 1-3 3h-1zM13 2a1 1 0 0 1 0-2 4 4 0 0 1 4 4v2a4 4 0 0 1-4 4 1 1 0 0 1 0-2 2 2 0 0 0 2-2V4a2 2 0 0 0-2-2z"/>
      </svg>
    </a>
    <div class="border-t border-slate-200 h-px"></div>
    <%# Plugin: Goals %>
    <a href="/dashboard/goals" class="p-2 bg-slate-100 hover:bg-slate-200 rounded-md transition-colors block" data-tooltip="Goals" data-tooltip-orientation="right" data-tooltip-offset="10">
      <svg viewBox="-2 -4 24 24" class="h-6 w-6" fill="currentColor">
        <path d="M3.636 7.208L10 13.572l6.364-6.364a3 3 0 1 0-4.243-4.243L10 5.086l-2.121-2.12a3 3 0 0 0-4.243 4.242zM9.293 1.55l.707.707.707-.707a5 5 0 1 1 7.071 7.071l-7.07 7.071a1 1 0 0 1-1.415 0l-7.071-7.07a5 5 0 1 1 7.07-7.071z"></path>
      </svg>
    </a>
  </div>
  <% } %>
  <div class="space-y-3">
    <%# Settings %>
    <a href="/dashboard/settings" class="p-2 bg-slate-100 hover:bg-slate-200 block rounded-md transition-colors" data-tooltip="Settings" data-tooltip-orientation="right" data-tooltip-offset="10">
      <svg viewBox="-2 -2 24 24" class="h-6 w-6" fill="currentColor">
        <path d="M20 8.163A2.106 2.106 0 0 0 18.926 10c0 .789.433 1.476 1.074 1.837l-.717 2.406a2.105 2.105 0 0 0-2.218 3.058l-2.062 1.602A2.104 2.104 0 0 0 11.633 20l-3.29-.008a2.104 2.104 0 0 0-3.362-1.094l-2.06-1.615A2.105 2.105 0 0 0 .715 14.24L0 11.825A2.106 2.106 0 0 0 1.051 10C1.051 9.22.63 8.54 0 8.175L.715 5.76a2.105 2.105 0 0 0 2.207-3.043L4.98 1.102A2.104 2.104 0 0 0 8.342.008L11.634 0a2.104 2.104 0 0 0 3.37 1.097l2.06 1.603a2.105 2.105 0 0 0 2.218 3.058L20 8.162zM14.823 3.68c0-.063.002-.125.005-.188l-.08-.062a4.103 4.103 0 0 1-4.308-1.428l-.904.002a4.1 4.1 0 0 1-4.29 1.43l-.095.076A4.108 4.108 0 0 1 2.279 7.6a4.1 4.1 0 0 1 .772 2.399c0 .882-.28 1.715-.772 2.4a4.108 4.108 0 0 1 2.872 4.09l.096.075a4.104 4.104 0 0 1 4.289 1.43l.904.002a4.1 4.1 0 0 1 4.307-1.428l.08-.062A4.108 4.108 0 0 1 17.7 12.4a4.102 4.102 0 0 1-.773-2.4c0-.882.281-1.716.773-2.4a4.108 4.108 0 0 1-2.876-3.919zM10 14a4 4 0 1 1 0-8 4 4 0 0 1 0 8zm0-2a2 2 0 1 0 0-4 2 2 0 0 0 0 4z"></path>
      </svg>
